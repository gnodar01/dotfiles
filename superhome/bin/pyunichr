#!python

"""
int to unicode codepoint
"""

# print out Private Use Area:
#   https://en.wikipedia.org/wiki/Private_Use_Areas
#
# for (( i=0xE000; i<0xEEA0; i++ )); do printf "%s " $(echo -ne $i | pyunichr); done; printf "\n"
#
# Nerd Font ranges documented here:
#   https://github.com/ryanoasis/nerd-fonts/wiki/Glyph-Sets-and-Code-Points#overview

import sys

contents = sys.stdin.read()

if contents.isdecimal():
    contents = int(contents)
else:
    sys.stderr.write(f"{contents} is not a decimal\n")
    exit(1)

if contents < 0:
    sys.stderr.write(f"{contents} is not a positive int\n")
    exit(1)

if contents >= 0x110000:
    sys.stderr.write(f"{contents} is out of range\n")
    exit(1)

contents = chr(contents)

sys.stdout.write(contents)
