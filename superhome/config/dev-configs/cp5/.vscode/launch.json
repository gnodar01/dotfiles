{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CellProfiler",
      "type": "python",
      "request": "launch",
      "python": "${workspaceFolder}/CellProfiler/.pixi/envs/hack/python.app/Contents/MacOS/python",
      "justMyCode": false,
      "module": "cellprofiler",
      "args": [
