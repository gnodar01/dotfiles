{
  "explorer.autoReveal": false,
  "outline.collapseItems": "alwaysCollapse",
  "files.associations": {
    "mac_javabridge_utils.h": "c",
    "jni.h": "c"
  },
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.automationProfile.osx": {},
  "python.defaultInterpreterPath": "",
  "python.analysis.extraPaths": [
    "./CellProfiler/src/frontend",
    "./CellProfiler/src/frontend/cellprofiler",
    "./CellProfiler/src/subpackages/core",
    "./CellProfiler/src/subpackages/library",
    "./centrosome"
  ],
  "python.analysis.typeCheckingMode": "off",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.pytestPath": "pytest",
  "python.testing.pytestArgs": [
    "CellProfiler/tests/library",
    "CellProfiler/tests/core",
    "CellProfiler/tests/frontend"
  ],
  "python.terminal.activateEnvironment": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.experiments.enabled": false,
  "testing.defaultGutterClickAction": "run",
  "testing.showCoverageInExplorer": false,
  "jupyter.debugJustMyCode": true,
  "jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
  ]
}
